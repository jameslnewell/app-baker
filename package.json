{
  "name": "tradie",
  "version": "0.8.1-preview.1",
  "description": "A semi-opinionated build tool for frontend projects. Use it to lint, bundle and test your script and style files.",
  "keywords": [
    "tradie",
    "build",
    "tool",
    "bundle",
    "bundler",
    "lint",
    "test",
    "browserify",
    "sass-composer",
    "eslint",
    "mocha"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:jameslnewell/tradie.git"
  },
  "dependencies": {
    "autoprefixer": "^6.3.3",
    "browserify": "^13.0.0",
    "browserify-incremental": "^3.1.1",
    "chalk": "^1.1.1",
    "clean-css": "^3.4.8",
    "envify": "^3.4.0",
    "eslint": "^1.10.3",
    "file-size": "^1.0.0",
    "glob": "^7.0.3",
    "humanize-duration": "^3.6.1",
    "json5": "^0.5.0",
    "lodash.mergewith": "^4.3.0",
    "make-error-cause": "^1.0.2",
    "mem-fs": "^1.1.2",
    "mem-fs-editor": "^2.1.0",
    "mkdirp": "^0.5.1",
    "mocaccino": "^1.8.2",
    "mocha": "^2.4.5",
    "postcss": "^5.0.16",
    "promised-del": "^1.0.2",
    "promisepipe": "^1.0.1",
    "resolve": "^1.1.7",
    "sass-composer": "^2.0.0-beta14",
    "source-map-support": "^0.4.0",
    "source-mapper": "^2.0.0",
    "through2": "^2.0.1",
    "uglify-js": "^2.6.2",
    "watchify": "^3.7.0",
    "yargs": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "eslint-config-jameslnewell": "0.4.3",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.3"
  },
  "bin": {
    "tradie": "./dist/bin/tradie.js"
  },
  "scripts": {
    "lint": "eslint src/**/*.js",
    "build": "babel -s --out-dir ./dist/ src",
    "test": "mocha $(find ./dist -name '*.test.js')",
    "prepublish": "npm run lint && npm run build && npm run test"
  },
  "license": "MIT"
}
