{
  "name": "tradie",
  "version": "1.2.0-preview.3",
  "description": "A semi-opinionated build tool for frontend projects. Use it to lint, bundle and test your script and style files.",
  "keywords": [
    "tradie",
    "build",
    "tool",
    "bundle",
    "bundler",
    "lint",
    "test",
    "browserify",
    "sass-composer",
    "eslint",
    "mocha"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:jameslnewell/tradie.git"
  },
  "dependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.13.2",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "ext-to-regex": "^0.1.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "file-name": "^0.1.0",
    "file-size": "^1.0.0",
    "find-and-read-package-json": "^1.0.0",
    "glob": "^7.1.1",
    "humanize-duration": "^3.10.0",
    "json-loader": "^0.5.4",
    "json5": "^0.5.1",
    "lodash.every": "^4.6.0",
    "lodash.mergewith": "^4.6.0",
    "make-error-cause": "^1.2.2",
    "memory-fs": "^0.4.1",
    "minimatch": "^3.0.3",
    "mocha": "^3.2.0",
    "node-noop": "^1.0.0",
    "node-sass": "^4.5.0",
    "object-values": "^1.0.0",
    "postcss": "^5.2.12",
    "postcss-asset-webpack-plugin": "^1.0.0",
    "postcss-loader": "^1.2.2",
    "promised-del": "^1.0.2",
    "resolve": "^1.2.0",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "source-map-support": "^0.4.11",
    "source-mapper": "^2.0.0",
    "style-loader": "^0.13.1",
    "supports-color": "^3.2.3",
    "webpack": "^2.2.1",
    "webpack-bundle-update-hook-plugin": "^1.0.2",
    "webpack-resolve-short-path-plugin": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "bootstrap": "^3.3.7",
    "chai": "^3.5.0",
    "chai-files": "^1.4.0",
    "eslint-config-jameslnewell": "^1.0.0-rc.9",
    "sinon": "^1.17.7"
  },
  "bin": {
    "tradie": "./dist/cli/index.js"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "lint": "eslint src/**/*.js",
    "build": "babel -s --out-dir ./dist/ src",
    "watch": "npm run build -- --watch",
    "test:unit": "mocha --require ./dist/_.test.js $(find ./dist -name '*.test.js')",
    "test:acceptance": "mocha --compilers js:babel-register --require ./dist/_.test.js",
    "test": "npm run test:unit",
    "prepublish": "npm run lint && npm run clean && npm run build && npm run test"
  },
  "license": "MIT"
}
